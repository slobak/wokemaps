{
  "manifest_version": 3,
  "name": "#wokemaps",
  "version": "0.9.2",
  "description": "Modifies Google Maps to restore progressive labels as overlays on some locations, for your relief and enjoyment.",
  "permissions": ["storage"],
  "host_permissions": [
    "*://*.google.com/maps/*"
  ],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "#wokemaps",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.google.com/maps/*"],
      "world": "MAIN",
      "js": ["content-page.js"],
      "run_at": "document_start"
    }, {
      "matches": ["*://*.google.com/maps/*"],
      "js": [
        "options-manager.js",
        "app-data-manager.js",
        "announcement-manager.js",
        "map-canvas.js",
        "map-state.js",
        "label-renderer.js",
        "overlay-engine.js",
        "content.js"
      ],
      "css": ["styles.css"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["default-options.json", "labels-v1.json", "images/icon16.png"],
      "matches": ["*://*.google.com/*"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}

