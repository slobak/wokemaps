{
  "manifest_version": 3,
  "name": "#wokemaps",
  "version": "0.9.6",
  "description": "Modifies Google Maps to restore progressive labels as overlays on some locations, for your relief and enjoyment.",
  "permissions": ["storage"],
  "host_permissions": [
    "*://*.google.com/maps/*",
    "*://wokemaps.org/*",
    "*://*.wokemaps.org/*"
  ],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "#wokemaps",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.google.com/maps/*"],
      "world": "MAIN",
      "js": [
        "logger-in-page.js",
        "content-in-google-maps.js"
      ],
      "run_at": "document_start"
    }, {
      "matches": ["*://*.google.com/maps/*"],
      "js": [
        "logger.js",
        "util.js",
        "options-manager.js",
        "uuid-manager.js",
        "app-data-manager.js",
        "announcement-manager.js",
        "map-canvas.js",
        "map-state-2d.js",
        "map-state-webgl.js",
        "label-renderer.js",
        "overlay-engine.js",
        "canvas-factory.js",
        "content-for-google-maps.js"
      ],
      "css": ["styles.css"],
      "run_at": "document_start"
    }, {
      "matches": ["*://wokemaps.org/*", "*://*.wokemaps.org/*"],
      "world": "MAIN",
      "js": ["content-in-wokemaps.js"],
      "run_at": "document_start"
    }, {
      "matches": ["*://wokemaps.org/*", "*://*.wokemaps.org/*"],
      "js": [
        "uuid-manager.js",
        "content-for-wokemaps.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["default-options.json", "app-data-v1.json", "images/icon16.png"],
      "matches": ["*://*.google.com/*"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}

